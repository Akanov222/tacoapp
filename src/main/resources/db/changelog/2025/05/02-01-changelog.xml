<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.29.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1746139153084-1" author="Administrator">
        <createTable tableName="taco_ingredients">
            <column name="ingredient_id" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="taco_id" type="BIGINT">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
    <changeSet id="1746139153084-2" author="Administrator">
        <createTable tableName="tacos">
            <column autoIncrement="true" name="id" type="BIGINT">
                <constraints nullable="false" primaryKey="true" primaryKeyName="pk_tacos"/>
            </column>
            <column name="name" type="VARCHAR(255)"/>
        </createTable>
    </changeSet>
    <changeSet id="1746139153084-3" author="Administrator">
        <addForeignKeyConstraint baseColumnNames="ingredient_id" baseTableName="taco_ingredients"
                                 constraintName="fk_tacing_on_ingredient" referencedColumnNames="id"
                                 referencedTableName="ingredients"/>
    </changeSet>
    <changeSet id="1746139153084-4" author="Administrator">
        <addForeignKeyConstraint baseColumnNames="taco_id" baseTableName="taco_ingredients"
                                 constraintName="fk_tacing_on_taco" referencedColumnNames="id"
                                 referencedTableName="tacos"/>
    </changeSet>

</databaseChangeLog>